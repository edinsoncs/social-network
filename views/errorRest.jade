doctype html
html(lang="es", ng-app="")
  head
    meta(charset="UTF-8")
    meta(name="description", content="Viainti")
    title #{web}
    link(rel="stylesheet", href="../stylesheets/normalize.css")
    link(rel="stylesheet", href="../stylesheets/viainti.css")

    link(href='https://api.mapbox.com/mapbox.js/v2.3.0/mapbox.css', rel='stylesheet')

    script(src="https://code.jquery.com/jquery-1.11.2.min.js")
  
    script(src="https://api.mapbox.com/mapbox.js/v2.3.0/mapbox.js")


    script(src="../javascripts/viainti.js")
    script(src="../javascripts/viaintifrontend.js")

    script(src='../javascripts/app-via.js')

  body.BodyListHotel
    header.menuViainti.bgHeader(data-role="header")
      include header-principal.jade
      nav.linkMenu
        ul.contMenu
          li.liMenu
            a.aLinkMenu(href="/libro/") Inicio
          li.liMenu
            a.aLinkMenu(href="../hoteles/") Hoteles
          li.liMenu
            a.aLinkMenu(href="../restaurantes/",  class="aLinkMenu aMenuHover") Restaurantes
          li.liMenu
            a.aLinkMenu(href="../vuelos/") Viajes
    main
      section.contenidoPrincipal.containerNot
        div.content__Notfound
          figure.Notfound--Figure
            img(src="../../images/noRest.png", alt="")
        div.content__Message
          h2.title
            | En este lugar no</br>contamos con Restaurantes
